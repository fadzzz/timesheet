{
  "name": "timesheet-monorepo",
  "version": "1.0.0",
  "description": "Timesheet application with React frontend and Node.js backend",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd timesheet-app && npm run dev",
    "dev:backend": "cd timesheet-backend && npm run dev",
    "install:all": "npm run install:frontend && npm run install:backend",
    "install:frontend": "cd timesheet-app && npm install",
    "install:backend": "cd timesheet-backend && npm install",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd timesheet-app && npm run build",
    "build:backend": "cd timesheet-backend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "author": "",
  "license": "MIT"
}